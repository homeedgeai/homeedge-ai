{
  "cli": {
    "version": ">= 10.0.0",
    "appVersionSource": "remote",
    "requireCommit": false,
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "EXPO_PUBLIC_ENV": "development"
      },
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "simulator": false,
        "buildConfiguration": "Debug",
        "autoIncrement": true,
        "credentialsSource": "remote"
      }
    },
    "adhoc": {
      "distribution": "internal",
      "channel": "adhoc",
      "credentialsSource": "remote",
      "env": {
        "EXPO_PUBLIC_ENV": "staging"
      },
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release",
        "autoIncrement": true
      },
      "android": {
        "gradleCommand": ":app:bundleRelease"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_ENV": "staging"
      },
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release"
      },
      "android": {
        "gradleCommand": ":app:bundleRelease"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "channel": "production",
      "credentialsSource": "remote",
      "env": {
        "EXPO_PUBLIC_ENV": "production"
      },
      "android": {
        "gradleCommand": ":app:bundleRelease"
      },
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "moorearmon24@gmail.com",
        "ascAppId": "6754081656",
        "appleTeamId": "2AX533DL45"
      },
      "android": {
        "serviceAccountKeyPath": "./credentials/android-service-account.json"
      }
    }
  }
}
