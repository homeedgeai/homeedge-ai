{
  "cli": {
    "version": ">= 10.0.0",
    "appVersionSource": "remote",
    "requireCommit": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "EXPO_PUBLIC_ENV": "development"
      },
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "simulator": false,
        "buildConfiguration": "Debug"
      }
    },
    "adhoc": {
      "distribution": "internal",
      "channel": "adhoc",
      "credentialsSource": "remote",
      "env": {
        "EXPO_PUBLIC_ENV": "release"
      },
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release"
      },
      "android": {
        "gradleCommand": ":app:bundleRelease"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_ENV": "staging"
      },
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_ENV": "production"
      },
      "android": {
        "gradleCommand": ":app:bundleRelease"
      },
      "ios": {
        "simulator": false
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "vogelmeplz12@gmail.com",
        "ascAppId": "6754081656"
      },
      "android": {
        "serviceAccountKeyPath": "./credentials/android-service-account.json"
      }
    }
  }
}
